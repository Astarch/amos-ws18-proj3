{
  "name": "pretrendr",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Patrick Willmann <patrick.m.willmann@gmail.com>",
  "private": true,
  "config": {
    "port": 3000
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_APP_MODE=app webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production NODE_APP_MODE=app webpack --progress --hide-modules",
    "unit": "cross-env BABEL_ENV=test jest ",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs"
  },
  "dependencies": {
    "vue": "^2.4.2",
    "vue-router": "^2.7.0"
  },
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-preset-env": "^1.6.0",
    "bootstrap": "3.3.7",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "d3": "^4.1.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "git-rev-sync": "^1.9.1",
    "gitlog": "^3.1.0",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "jest": "^22.0.3",
    "jest-html-reporter": "^0.5.7",
    "jest-vue-preprocessor": "^1.3.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.4",
    "lolex": "^1.5.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.0.0",
    "phantomjs-prebuilt": "^2.1.14",
    "popper.js": "^1.12.6",
    "qs": "^6.5.1",
    "sass-loader": "^6.0.6",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-resource": "^1.3.4",
    "vue-template-compiler": "^2.4.2",
    "vue-test-utils": "^1.0.0-beta.8",
    "webpack": "^3.5.4",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "globals": {
      "GIT": {}
    },
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "src(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "mapCoverage": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "coverageDirectory": "<rootDir>/test/coverage",
    "testResultsProcessor": "./node_modules/jest-html-reporter"
  },
  "jest-html-reporter": {
    "outputPath": "test/report/index.html"
  }
}
